{
  "name": "@mcp-everything/backend",
  "version": "1.0.0",
  "description": "NestJS backend for MCP Everything - AI-native platform for generating MCP servers",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate-mcp": "npm run build && node dist/scripts/generate-mcp.js",
    "docker:build": "npm run build && node dist/scripts/docker-build.js",
    "docker:test": "npm run build && node dist/scripts/docker-test.js",
    "test:anthropic": "ts-node src/scripts/test-anthropic-integration.ts",
    "test:anthropic:built": "npm run build && node dist/scripts/test-anthropic-integration.js",
    "test:anthropic:simple": "ts-node src/scripts/test-anthropic-simple.ts",
    "test:github-analysis": "ts-node src/scripts/test-github-analysis.ts",
    "test:github-analysis:built": "npm run build && node dist/scripts/test-github-analysis.js",
    "test:tool-discovery": "ts-node src/scripts/test-tool-discovery.ts",
    "test:tool-discovery:built": "npm run build && node dist/scripts/test-tool-discovery.js",
    "test:mcp-generation": "ts-node src/scripts/test-mcp-generation.ts",
    "test:mcp-generation:built": "npm run build && node dist/scripts/test-mcp-generation.js",
    "migration:run": "typeorm migration:run -d dist/database/data-source.js",
    "migration:revert": "typeorm migration:revert -d dist/database/data-source.js",
    "migration:generate": "typeorm migration:generate -d dist/database/data-source.js",
    "migration:create": "typeorm migration:create",
    "migration:show": "typeorm migration:show -d dist/database/data-source.js",
    "seed": "node dist/database/seeds/run-seeds.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.1",
    "@langchain/anthropic": "^0.3.30",
    "@langchain/core": "^0.3.78",
    "@langchain/langgraph": "^0.4.9",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@octokit/rest": "^20.0.2",
    "@types/libsodium-wrappers": "^0.7.14",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "dockerode": "^4.0.2",
    "helmet": "^7.1.0",
    "isolated-vm": "^5.0.4",
    "joi": "^17.12.0",
    "js-yaml": "^4.1.0",
    "langchain": "^0.3.35",
    "libsodium-wrappers": "^0.7.15",
    "nanoid": "^5.1.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stripe": "^20.0.0",
    "tar-fs": "^3.0.4",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.8",
    "@types/node": "^22.7.5",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "compiler": "typescript",
          "tsconfig": {
            "module": "commonjs",
            "types": [
              "jest",
              "node"
            ],
            "emitDecoratorMetadata": true,
            "experimentalDecorators": true,
            "allowSyntheticDefaultImports": true,
            "esModuleInterop": true
          }
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "nestjs",
    "mcp",
    "model-context-protocol",
    "ai",
    "docker",
    "github",
    "anthropic"
  ],
  "author": "MCP Everything Team",
  "license": "MIT"
}
